<h1>Edit User</h1>
<%= form_for @user do |f| %>
  <%= f.label :username, 'Your name here:   ' %>
  <%= f.text_field :username, placeholder: "username", value: "Mike" %><br><br>
  <%= f.label :email, 'Your email here:   ' %>
  <%= f.text_field :email, placeholder: "email", value: "mike@mike.mike" %><br><br>
  <%= f.label :password, 'Your password here:   ' %>
  <%= f.text_field :password, placeholder: "password", value: "12345" %><br><br>
  <%= f.submit "Submit" %>
<% end %>
